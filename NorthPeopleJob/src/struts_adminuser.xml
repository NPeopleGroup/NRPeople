<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="adminUserAction" extends="struts-default,json-default"
		namespace="/">
		<action name="adminUserList">
			<result name="success">/WEB-INF/admin/adminuser_list.jsp
			</result>
		</action>
		<action name="adminUserAdd">
			<result name="success">/WEB-INF/admin/adminuser_add.jsp
			</result>
		</action>
		<action name="addAdminUser" class="com.hyd.northpj.action.AdminUserOpeAction" method="userAdd">
			<result name="success">/WEB-INF/admin/adminuser_add.jsp
			</result>
		</action>
		<action name="deleteAdminUser" class="com.hyd.northpj.action.AdminUserOpeAction" method="userDelete">
			<result name="success">/WEB-INF/admin/adminuser_add.jsp
			</result>
		</action>
		<action name="updateAdminUser" class="com.hyd.northpj.action.AdminUserOpeAction" method="updateDelete">
			<result name="success">/WEB-INF/admin/adminuser_add.jsp
			</result>
		</action>

	</package>
</struts>